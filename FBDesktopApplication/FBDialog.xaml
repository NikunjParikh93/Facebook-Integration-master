<Window x:Class="FBDesktopApplication.FBDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FBDesktopApplication"
        mc:Ignorable="d"
        Title="FBDialog" Height="300" Width="300" Loaded="Window_Loaded">    
    <Grid>
        <WebBrowser x:Name="FbdialogWebBrowser" LoadCompleted="Fbdialog_LoadCompleted" Navigated="Fbdialog_Navigated" ></WebBrowser>
    </Grid>
</Window>
